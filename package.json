{
  "name": "melnycz.uk",
  "version": "0.2.0",
  "main": "./pages/index.tsx",
  "private": true,
  "keywords": [
    "artist"
  ],
  "browser": {
    "fs": false,
    "os": false,
    "path": false,
    "child_process": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=18.17.0 <=21.0.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "file-loader": "^1.1.6",
    "front-matter": "^4.0.2",
    "modern-css-reset": "^1.3.0",
    "nano-markdown": "^1.2.2",
    "next": "^14.0.2",
    "raw-loader": "^0.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-progressive-image": "^0.6.0",
    "react-use": "^14.2.0",
    "remark": "^13.0.0",
    "remark-react": "^8.0.0",
    "rss": "^1.2.2",
    "sass": "^1.32.2",
    "stream": "^0.0.2",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^20.9.0",
    "@types/react": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@typescript-eslint/typescript-estree": "^2.33.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "follow-redirects": "^1.15.4",
    "husky": "^4.2.3",
    "imagemagick": "^0.1.3",
    "jest": "^29.7.0",
    "lint-staged": "^10.1.1",
    "prettier": "^2.2.1",
    "serve": "^14.2.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.ts"
    ],
    "testMatch": [
      "**/*.test.tsx"
    ],
    "verbose": false
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn format",
      "yarn lint:fix"
    ]
  },
  "resolutions": {
    "@typescript-eslint/parser": "^1.11.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .next/cache && rm -rf out",
    "dev": "next dev",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --single-quote --trailing-comma es5 --write './src/**/*.{js,ts,tsx}'",
    "sass": "sass ./src/styles/index.scss ./out/index.css && rm ./out/index.css.map",
    "scripts": "node",
    "serve": "serve out -p 3000",
    "test": "echo 'no tests'",
    "typecheck": "tsc --noEmit"
  }
}
