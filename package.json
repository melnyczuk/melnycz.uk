{
  "name": "melnycz.uk",
  "version": "0.2.0",
  "main": "./pages/index.tsx",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-manifest": "^2.10.0",
    "gatsby-plugin-offline": "^3.8.0",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-sass": "^3.0.0",
    "gatsby-plugin-sharp": "^2.12.0",
    "gatsby-plugin-sitemap": "^2.10.0",
    "gatsby-plugin-typescript": "^2.10.0",
    "gatsby-source-filesystem": "^2.9.0",
    "gatsby-source-graphql": "^2.12.0",
    "gatsby-transformer-remark": "^2.14.0",
    "gatsby-transformer-sharp": "^2.10.0",
    "modern-css-reset": "^1.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-use": "^14.2.0",
    "remark": "^13.0.0",
    "remark-react": "^8.0.0",
    "sass": "^1.32.2",
    "sharp": "^0.27.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@typescript-eslint/typescript-estree": "^2.33.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.3",
    "jest": "^24.8.0",
    "lint-staged": "^10.1.1",
    "prettier": "^2.2.1",
    "ts-jest": "^24.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.ts"
    ],
    "testMatch": [
      "**/*.test.tsx"
    ],
    "verbose": false
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint:fix",
      "yarn prettier",
      "git add"
    ]
  },
  "resolutions": {
    "@typescript-eslint/parser": "^1.11.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --single-quote --trailing-comma es5 --write './src/**/*.{js,ts,tsx}'",
    "sass": "sass ./src/styles/index.scss ./out/index.css && rm ./out/index.css.map",
    "serve": "gatsby serve",
    "typecheck": "tsc --noEmit"
  }
}
