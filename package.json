{
  "name": "melnycz.uk",
  "version": "0.2.0",
  "main": "./pages/index.tsx",
  "private": true,
  "keywords": [
    "artist"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@apollo/react-hoc": "^4.0.0",
    "classnames": "^2.3.1",
    "graphql": "^15.5.0",
    "modern-css-reset": "^1.3.0",
    "next": "^10.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-progressive-image": "^0.6.0",
    "react-use": "^14.2.0",
    "remark": "^13.0.0",
    "remark-react": "^8.0.0",
    "sass": "^1.32.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@typescript-eslint/typescript-estree": "^2.33.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.3",
    "jest": "^24.8.0",
    "lint-staged": "^10.1.1",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "ts-jest": "^24.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.ts"
    ],
    "testMatch": [
      "**/*.test.tsx"
    ],
    "verbose": false
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn format",
      "git add"
    ]
  },
  "resolutions": {
    "@typescript-eslint/parser": "^1.11.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "dist": "yarn build && yarn export",
    "export": "next export",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --single-quote --trailing-comma es5 --write './src/**/*.{js,ts,tsx}'",
    "sass": "sass ./src/styles/index.scss ./out/index.css && rm ./out/index.css.map",
    "serve": "serve out",
    "test": "echo 'no tests'",
    "typecheck": "tsc --noEmit"
  }
}
